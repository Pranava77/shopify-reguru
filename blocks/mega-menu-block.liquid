{%- comment -%}
  AI-generated mega menu block with 5 configurable items
{%- endcomment -%}

{% stylesheet %}
.ai-mega-menu-block {
  display: block;
  width: 100%;
  background-color: var(--mm-bg-color);
  border-bottom: 1px solid var(--mm-border-color);
}

.ai-mega-menu-block__container {
  max-width: var(--mm-max-width);
  margin: 0 auto;
  padding: 0 20px;
}

.ai-mega-menu-block__nav {
  display: flex;
  justify-content: var(--mm-alignment);
  gap: var(--mm-item-spacing);
  list-style: none;
  margin: 0;
  padding: 0;
}

.ai-mega-menu-block__item {
  position: relative;
}

.ai-mega-menu-block__link {
  display: flex;
  align-items: center;
  gap: 6px;
  padding: var(--mm-v-padding) var(--mm-h-padding);
  color: var(--mm-text-color);
  text-decoration: none;
  font-size: var(--mm-font-size);
  font-weight: var(--mm-font-weight);
  transition: color 0.3s ease;
}

.ai-mega-menu-block__link:hover {
  color: var(--mm-text-hover-color);
}

.ai-mega-menu-block__icon {
  width: 12px;
  height: 12px;
  transition: transform 0.3s ease;
}

.ai-mega-menu-block__item:hover .ai-mega-menu-block__icon {
  transform: rotate(180deg);
}

.ai-mega-menu-block__dropdown {
  position: absolute;
  top: 100%;
  left: 50%;
  transform: translateX(-50%);
  width: var(--mm-dropdown-width);
  background-color: var(--mm-dropdown-bg-color);
  border: 1px solid var(--mm-border-color);
  border-radius: var(--mm-dropdown-border-radius);
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
  opacity: 0;
  visibility: hidden;
  transition: opacity 0.3s ease, visibility 0.3s ease, transform 0.3s ease;
  transform: translateX(-50%) translateY(10px);
  z-index: 1000;
  padding: var(--mm-dropdown-padding);
}

.ai-mega-menu-block__item:hover .ai-mega-menu-block__dropdown {
  opacity: 1;
  visibility: visible;
  transform: translateX(-50%) translateY(0);
}

.ai-mega-menu-block__dropdown-content {
  display: grid;
  grid-template-columns: repeat(var(--mm-dropdown-columns), 1fr);
  gap: var(--mm-dropdown-gap);
}

.ai-mega-menu-block__column {
  display: flex;
  flex-direction: column;
  gap: 12px;
}

.ai-mega-menu-block__column-title {
  font-size: var(--mm-column-title-size);
  font-weight: 600;
  color: var(--mm-column-title-color);
  margin: 0 0 8px 0;
  padding-bottom: 8px;
  border-bottom: 2px solid var(--mm-border-color);
}

.ai-mega-menu-block__column-links {
  display: flex;
  flex-direction: column;
  gap: 8px;
  list-style: none;
  margin: 0;
  padding: 0;
}

.ai-mega-menu-block__column-link {
  color: var(--mm-dropdown-text-color);
  text-decoration: none;
  font-size: var(--mm-dropdown-link-size);
  transition: color 0.3s ease, padding-left 0.3s ease;
}

.ai-mega-menu-block__column-link:hover {
  color: var(--mm-dropdown-link-hover-color);
  padding-left: 8px;
}

.ai-mega-menu-block__featured {
  grid-column: span var(--mm-dropdown-columns);
  margin-top: 16px;
  padding-top: 16px;
  border-top: 1px solid var(--mm-border-color);
}

.ai-mega-menu-block__featured-title {
  font-size: var(--mm-featured-title-size);
  font-weight: 600;
  color: var(--mm-column-title-color);
  margin: 0 0 12px 0;
}

.ai-mega-menu-block__featured-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
  gap: 16px;
}

.ai-mega-menu-block__featured-item {
  text-decoration: none;
  display: flex;
  flex-direction: column;
  gap: 8px;
}

.ai-mega-menu-block__featured-image {
  width: 100%;
  aspect-ratio: 1;
  object-fit: cover;
  border-radius: 8px;
}

.ai-mega-menu-block__featured-placeholder {
  width: 100%;
  aspect-ratio: 1;
  background-color: #f4f4f4;
  border-radius: 8px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.ai-mega-menu-block__featured-placeholder svg {
  width: 60%;
  height: 60%;
}

.ai-mega-menu-block__featured-name {
  font-size: 14px;
  color: var(--mm-dropdown-text-color);
  text-align: center;
}

/* Responsive Styles (Mobile-First) */
@media screen and (min-width: 990px) {
  .ai-mega-menu-block__nav {
    flex-direction: row;
  }
}

@media screen and (max-width: 989px) {
  .ai-mega-menu-block__nav {
    flex-direction: column;
    gap: 0;
  }

  .ai-mega-menu-block__dropdown {
    position: static;
    transform: none;
    width: 100%;
    border: none;
    border-radius: 0;
    box-shadow: none;
    padding: 0 0 0 20px;
    margin-top: 8px;
  }

  .ai-mega-menu-block__item:hover .ai-mega-menu-block__dropdown {
    transform: none;
  }

  .ai-mega-menu-block__dropdown-content {
    grid-template-columns: 1fr;
  }

  .ai-mega-menu-block__featured {
    grid-column: span 1;
  }
}
