<mega-menu-block
  class="ai-mega-menu-block"
  style="
    --mm-bg-color: {{ block.settings.background_color }};
    --mm-border-color: {{ block.settings.border_color }};
    --mm-max-width: {{ block.settings.max_width }}px;
    --mm-alignment: {{ block.settings.alignment }};
    --mm-item-spacing: {{ block.settings.item_spacing }}px;
    --mm-v-padding: {{ block.settings.vertical_padding }}px;
    --mm-h-padding: {{ block.settings.horizontal_padding }}px;
    --mm-text-color: {{ block.settings.text_color }};
    --mm-text-hover-color: {{ block.settings.text_hover_color }};
    --mm-font-size: {{ block.settings.font_size }}px;
    --mm-font-weight: {{ block.settings.font_weight }};
    --mm-dropdown-width: {{ block.settings.dropdown_width }}px;
    --mm-dropdown-bg-color: {{ block.settings.dropdown_bg_color }};
    --mm-dropdown-border-radius: {{ block.settings.dropdown_border_radius }}px;
    --mm-dropdown-padding: {{ block.settings.dropdown_padding }}px;
    --mm-dropdown-columns: {{ block.settings.dropdown_columns }};
    --mm-dropdown-gap: {{ block.settings.dropdown_gap }}px;
    --mm-column-title-size: {{ block.settings.column_title_size }}px;
    --mm-column-title-color: {{ block.settings.column_title_color }};
    --mm-dropdown-text-color: {{ block.settings.dropdown_text_color }};
    --mm-dropdown-link-size: {{ block.settings.dropdown_link_size }}px;
    --mm-dropdown-link-hover-color: {{ block.settings.dropdown_link_hover_color }};
    --mm-featured-title-size: {{ block.settings.featured_title_size }}px;
  "
  {{ block.shopify_attributes }}
  <div class="ai-mega-menu-block__container">
    <ul class="ai-mega-menu-block__nav">
      {% for i in (1..5) %}
        {% liquid
          assign menu_title_key = 'menu_' | append: i | append: '_title'
          assign menu_link_key = 'menu_' | append: i | append: '_link'
          assign show_dropdown_key = 'menu_' | append: i | append: '_show_dropdown'
          
          assign menu_title = block.settings[menu_title_key]
          assign menu_link = block.settings[menu_link_key]
          assign show_dropdown = block.settings[show_dropdown_key]
        %}
        
        {% if menu_title != blank %}
          <li class="ai-mega-menu-block__item">
            <a href="{{ menu_link }}" class="ai-mega-menu-block__link">
              {{ menu_title }}
              {% if show_dropdown %}
                <svg class="ai-mega-menu-block__icon" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M2 4L6 8L10 4" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
              {% endif %}
            </a>
            
            {% if show_dropdown %}
              <div class="ai-mega-menu-block__dropdown">
                <div class="ai-mega-menu-block__dropdown-content">
                  {% for col in (1..3) %}
                    {% liquid
                      assign col_title_key = 'menu_' | append: i | append: '_col_' | append: col | append: '_title'
                      assign col_link1_text_key = 'menu_' | append: i | append: '_col_' | append: col | append: '_link1_text'
                      assign col_link1_url_key = 'menu_' | append: i | append: '_col_' | append: col | append: '_link1_url'
                      assign col_link2_text_key = 'menu_' | append: i | append: '_col_' | append: col | append: '_link2_text'
                      assign col_link2_url_key = 'menu_' | append: i | append: '_col_' | append: col | append: '_link2_url'
                      assign col_link3_text_key = 'menu_' | append: i | append: '_col_' | append: col | append: '_link3_text'
                      assign col_link3_url_key = 'menu_' | append: i | append: '_col_' | append: col | append: '_link3_url'
                      
                      assign col_title = block.settings[col_title_key]
                      assign col_link1_text = block.settings[col_link1_text_key]
                      assign col_link1_url = block.settings[col_link1_url_key]
                      assign col_link2_text = block.settings[col_link2_text_key]
                      assign col_link2_url = block.settings[col_link2_url_key]
                      assign col_link3_text = block.settings[col_link3_text_key]
                      assign col_link3_url = block.settings[col_link3_url_key]
                    %}
                    
                    <div class="ai-mega-menu-block__column">
                      {% if col_title != blank %}
                        <h3 class="ai-mega-menu-block__column-title">{{ col_title }}</h3>
                      {% endif %}
                      <ul class="ai-mega-menu-block__column-links">
                        {% if col_link1_text != blank %}
                          <li>
                            <a href="{{ col_link1_url }}" class="ai-mega-menu-block__column-link">{{ col_link1_text }}</a>
                          </li>
                        {% endif %}
                        {% if col_link2_text != blank %}
                          <li>
                            <a href="{{ col_link2_url }}" class="ai-mega-menu-block__column-link">{{ col_link2_text }}</a>
                          </li>
                        {% endif %}
                        {% if col_link3_text != blank %}
                          <li>
                            <a href="{{ col_link3_url }}" class="ai-mega-menu-block__column-link">{{ col_link3_text }}</a>
                          </li>
                        {% endif %}
                      </ul>
                    </div>
                  {% endfor %}
                  
                  {% liquid
                    assign featured_title_key = 'menu_' | append: i | append: '_featured_title'
                    assign featured_product1_key = 'menu_' | append: i | append: '_featured_product1'
                    assign featured_product2_key = 'menu_' | append: i | append: '_featured_product2'
                    
                    assign featured_title = block.settings[featured_title_key]
                    assign featured_product1 = block.settings[featured_product1_key]
                    assign featured_product2 = block.settings[featured_product2_key]
                  %}
                  
                  {% if featured_title != blank or featured_product1 != blank or featured_product2 != blank %}
                    <div class="ai-mega-menu-block__featured">
                      {% if featured_title != blank %}
                        <h3 class="ai-mega-menu-block__featured-title">{{ featured_title }}</h3>
                      {% endif %}
                      <div class="ai-mega-menu-block__featured-grid">
                        {% if featured_product1 != blank %}
                          <a href="{{ featured_product1.url }}" class="ai-mega-menu-block__featured-item">
                            {% if featured_product1.featured_image %}
                              <img 
                                src="{{ featured_product1.featured_image | image_url: width: 300 }}" 
                                alt="{{ featured_product1.featured_image.alt | escape }}"
                                class="ai-mega-menu-block__featured-image"
                                loading="lazy"
                                width="300"
                                height="300"
                              >
                            {% else %}
                              <div class="ai-mega-menu-block__featured-placeholder">
                                {{ 'product-1' | placeholder_svg_tag }}
                              </div>
                            {% endif %}
                            <span class="ai-mega-menu-block__featured-name">{{ featured_product1.title }}</span>
                          </a>
                        {% endif %}
                        {% if featured_product2 != blank %}
                          <a href="{{ featured_product2.url }}" class="ai-mega-menu-block__featured-item">
                            {% if featured_product2.featured_image %}
                              <img 
                                src="{{ featured_product2.featured_image | image_url: width: 300 }}" 
                                alt="{{ featured_product2.featured_image.alt | escape }}"
                                class="ai-mega-menu-block__featured-image"
                                loading="lazy"
                                width="300"
                                height="300"
                              >
                            {% else %}
                              <div class="ai-mega-menu-block__featured-placeholder">
                                {{ 'product-1' | placeholder_svg_tag }}
                              </div>
                            {% endif %}
                            <span class="ai-mega-menu-block__featured-name">{{ featured_product2.title }}</span>
                          </a>
                        {% endif %}
                      </div>
                    </div>
                  {% endif %}
                </div>
              </div>
            {% endif %}
          </li>
        {% endif %}
      {% endfor %}
    </ul>
  </div>
</mega-menu-block>

<script>
  (function() {
    class MegaMenuBlock extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.menuItems = this.querySelectorAll('.ai-mega-menu-block__item');
        this.setupAccessibility();
        this.setupMobileToggle();
      }

      setupAccessibility() {
        for (const item of this.menuItems) {
          const link = item.querySelector('.ai-mega-menu-block__link');
          const dropdown = item.querySelector('.ai-mega-menu-block__dropdown');
          
          if (dropdown) {
            link.setAttribute('aria-haspopup', 'true');
            link.setAttribute('aria-expanded', 'false');
            
            item.addEventListener('mouseenter', () => {
              link.setAttribute('aria-expanded', 'true');
            });
            
            item.addEventListener('mouseleave', () => {
              link.setAttribute('aria-expanded', 'false');
            });
            
            link.addEventListener('click', (e) => {
              if (window.innerWidth <= 989) {
                e.preventDefault();
                const isExpanded = link.getAttribute('aria-expanded') === 'true';
                link.setAttribute('aria-expanded', !isExpanded);
                dropdown.style.opacity = !isExpanded ? '1' : '0';
                dropdown.style.visibility = !isExpanded ? 'visible' : 'hidden';
              }
            });
          }
        }
      }

      setupMobileToggle() {
        window.addEventListener('resize', () => {
          if (window.innerWidth > 989) {
            for (const item of this.menuItems) {
              const dropdown = item.querySelector('.ai-mega-menu-block__dropdown');
              if (dropdown) {
                dropdown.style.opacity = '';
                dropdown.style.visibility = '';
              }
            }
          }
        });
      }
    }

    if (!customElements.get('mega-menu-block')) {
      customElements.define('mega-menu-block', MegaMenuBlock);
    }
  })();
</script>

