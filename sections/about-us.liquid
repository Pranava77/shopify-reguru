{%- liquid
  assign hero_title_line_1 = section.settings.hero_title_line_1 | default: 'Beyond Refurbishment'
  assign hero_title_line_2 = section.settings.hero_title_line_2 | default: 'Redefining Smart'
  assign hero_title_line_3 = section.settings.hero_title_line_3 | default: 'Tech Ownership'
  assign hero_description = section.settings.hero_description | default: 'ReGuru is building a sustainable, circular tech ecosystem — making technology accessible while reducing e-waste.'
  assign hero_button_text = section.settings.hero_button_text | default: 'Join Franchise'
  assign hero_button_link = section.settings.hero_button_link | default: '#'
  assign hero_image = section.settings.hero_image
  
  assign franchise_card_title = section.settings.franchise_card_title | default: 'ReGuru Franchise'
  assign franchise_card_description = section.settings.franchise_card_description | default: "Build a high-growth tech retail business with India's most trusted refurbished electronics brand."
  assign franchise_card_button_text = section.settings.franchise_card_button_text | default: 'join franchise'
  assign franchise_card_button_link = section.settings.franchise_card_button_link | default: '#'
  
  assign brands_title = section.settings.brands_title | default: 'Top Brands  we sell:'
  
  assign expertise_title = section.settings.expertise_title | default: 'Our expertise'
  assign expertise_description = section.settings.expertise_description | default: 'Lorem ipsum dolor sit amet consectetur adipisicing elit. Quisquam, quos.'
  assign expertise_image_1 = section.settings.expertise_image_1
  assign expertise_text_1 = section.settings.expertise_text_1
  assign expertise_link_1 = section.settings.expertise_link_1
  assign expertise_image_2 = section.settings.expertise_image_2
  assign expertise_text_2 = section.settings.expertise_text_2
  assign expertise_link_2 = section.settings.expertise_link_2
  
  assign bg_image = section.settings.background_image
-%}

<section class="about-us" data-section-id="{{ section.id }}"{% if bg_image != blank %} style="background-image: url('{{ bg_image | image_url: width: 1920 }}'); background-size: cover; background-position: center; background-repeat: no-repeat;"{% endif %}>
  <!-- Background Grid Patterns -->
  <div class="about-us__grid-pattern about-us__grid-pattern--top"></div>
  <div class="about-us__grid-pattern about-us__grid-pattern--bottom"></div>
  
  <div class="about-us__container">
    <!-- Hero Section -->
    <div class="about-us__hero">
      <div class="about-us__hero-content">
        <div class="about-us__hero-text">
          <h1 class="about-us__hero-title">
            <span class="about-us__hero-title--blue">{{ hero_title_line_1 }}</span>
            <span class="about-us__hero-title--black">{{ hero_title_line_2 }}</span>
            <span class="about-us__hero-title--black">{{ hero_title_line_3 }}</span>
          </h1>
          <p class="about-us__hero-description">{{ hero_description }}</p>
          <a href="{{ hero_button_link }}" class="about-us__hero-button">
            {{ hero_button_text }}
          </a>
        </div>
        
        <div class="about-us__brands">
          <p class="about-us__brands-title">{{ brands_title }}</p>
          <div class="about-us__brands-list">
            {%- for block in section.blocks -%}
              {%- if block.type == 'brand_logo' -%}
                <div class="about-us__brand-item" {{ block.shopify_attributes }}>
                  {%- if block.settings.brand_logo != blank -%}
                    <img 
                      src="{{ block.settings.brand_logo | image_url: width: 120 }}"
                      alt="{{ block.settings.brand_name | default: 'Brand logo' }}"
                      width="120"
                      height="40"
                      loading="lazy"
                      class="about-us__brand-logo"
                    >
                  {%- else -%}
                    <span class="about-us__brand-placeholder">{{ block.settings.brand_name | default: 'logo ' | append: forloop.index }}</span>
                  {%- endif -%}
                </div>
              {%- endif -%}
            {%- endfor -%}
            
            {%- if section.blocks.size == 0 -%}
              {%- assign default_brands = 'logo one|logo two|logo three|logo four' | split: '|' -%}
              {%- for brand in default_brands -%}
                <div class="about-us__brand-item">
                  <span class="about-us__brand-placeholder">{{ brand }}</span>
                </div>
              {%- endfor -%}
            {%- endif -%}
          </div>
        </div>
      </div>
      
      <div class="about-us__hero-visual">
        <div class="about-us__hero-image-wrapper">
          {%- if hero_image != blank -%}
            <img 
              src="{{ hero_image | image_url: width: 700 }}"
              alt="{{ hero_image.alt | default: hero_title_line_1 }}"
              width="700"
              height="746"
              loading="lazy"
              class="about-us__hero-image"
            >
          {%- else -%}
            <div class="about-us__hero-placeholder"></div>
          {%- endif -%}
        </div>
        
        <!-- Franchise Card Overlay -->
        <div class="about-us__franchise-card">
          <div class="about-us__franchise-card-content">
            <h3 class="about-us__franchise-card-title">{{ franchise_card_title }}</h3>
            <p class="about-us__franchise-card-description">{{ franchise_card_description }}</p>
            
            <ul class="about-us__franchise-card-features">
              {%- if section.settings.franchise_feature_1 != blank -%}
                <li class="about-us__franchise-card-feature">
                  <svg class="about-us__checkmark-icon" width="10" height="10" viewBox="0 0 10 10" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M8.5 2.5L4 7L1.5 4.5" stroke="#28D98E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                  </svg>
                  <span>{{ section.settings.franchise_feature_1 }}</span>
                </li>
              {%- endif -%}
              {%- if section.settings.franchise_feature_2 != blank -%}
                <li class="about-us__franchise-card-feature">
                  <svg class="about-us__checkmark-icon" width="10" height="10" viewBox="0 0 10 10" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M8.5 2.5L4 7L1.5 4.5" stroke="#28D98E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                  </svg>
                  <span>{{ section.settings.franchise_feature_2 }}</span>
                </li>
              {%- endif -%}
            </ul>
            
            <a href="{{ franchise_card_button_link }}" class="about-us__franchise-card-button">
              {{ franchise_card_button_text }}
            </a>
          </div>
        </div>
        
        <!-- Decorative Elements -->
        <div class="about-us__decorative-dot about-us__decorative-dot--1"></div>
        <div class="about-us__decorative-dot about-us__decorative-dot--2"></div>
        <div class="about-us__decorative-dot about-us__decorative-dot--3"></div>
        <div class="about-us__decorative-square about-us__decorative-square--1"></div>
        <div class="about-us__decorative-square about-us__decorative-square--2"></div>
      </div>
    </div>
    
    <!-- Expertise Section -->
    <div class="about-us__expertise">
      <div class="about-us__expertise-header">
        <h2 class="about-us__expertise-title">{{ expertise_title }}</h2>
        <p class="about-us__expertise-description">{{ expertise_description }}</p>
      </div>
      
      <div class="about-us__expertise-content">
        <!-- Row 1: Image Left, Text Right -->
        <div class="about-us__expertise-row">
          <div class="about-us__expertise-image-wrapper">
            {%- if expertise_image_1 != blank -%}
              <img 
                src="{{ expertise_image_1 | image_url: width: 440 }}"
                alt="{{ expertise_image_1.alt | default: expertise_title }}"
                width="440"
                height="279"
                loading="lazy"
                class="about-us__expertise-image"
              >
            {%- else -%}
              <div class="about-us__expertise-placeholder"></div>
            {%- endif -%}
          </div>
          <div class="about-us__expertise-text">
            {%- if expertise_text_1 != blank -%}
              {{ expertise_text_1 }}
            {%- else -%}
              <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.</p>
            {%- endif -%}
            {%- if expertise_link_1 != blank -%}
              <a href="{{ expertise_link_1 }}" class="about-us__expertise-link">Lorem ipsum</a>
            {%- endif -%}
          </div>
        </div>
        
        <!-- Row 2: Text Left, Image Right -->
        <div class="about-us__expertise-row about-us__expertise-row--reverse">
          <div class="about-us__expertise-text">
            {%- if expertise_text_2 != blank -%}
              {{ expertise_text_2 }}
            {%- else -%}
              <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.</p>
            {%- endif -%}
            {%- if expertise_link_2 != blank -%}
              <a href="{{ expertise_link_2 }}" class="about-us__expertise-link">Lorem ipsum</a>
            {%- endif -%}
          </div>
          <div class="about-us__expertise-image-wrapper">
            {%- if expertise_image_2 != blank -%}
              <img 
                src="{{ expertise_image_2 | image_url: width: 440 }}"
                alt="{{ expertise_image_2.alt | default: expertise_title }}"
                width="440"
                height="279"
                loading="lazy"
                class="about-us__expertise-image"
              >
            {%- else -%}
              <div class="about-us__expertise-placeholder"></div>
            {%- endif -%}
          </div>
        </div>
      </div>
      
      <!-- Laptop Illustration -->
      <div class="about-us__laptop-illustration">
        {%- if section.settings.laptop_image != blank -%}
          <img 
            src="{{ section.settings.laptop_image | image_url: width: 240 }}"
            alt="Laptop illustration"
            width="183"
            height="171"
            loading="lazy"
            class="about-us__laptop-img"
          >
        {%- else -%}
          <svg class="about-us__laptop-img" viewBox="0 0 120 120" fill="none" xmlns="http://www.w3.org/2000/svg">
            <rect x="15" y="75" width="90" height="6" rx="2" fill="#4A5568" stroke="#000" stroke-width="1.5"/>
            <rect x="22" y="28" width="76" height="50" rx="3" fill="#2D3748" stroke="#000" stroke-width="1.5"/>
            <rect x="26" y="32" width="68" height="42" rx="1" fill="#E2E8F0"/>
            <rect x="32" y="40" width="35" height="3" rx="1" fill="#A0AEC0"/>
            <rect x="32" y="48" width="45" height="3" rx="1" fill="#A0AEC0"/>
            <rect x="32" y="56" width="28" height="3" rx="1" fill="#A0AEC0"/>
            <circle cx="73" cy="54" r="14" fill="#28D98E" stroke="#000" stroke-width="1.5"/>
            <path d="M66 54L71 59L81 49" stroke="#FFF" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/>
            <path d="M50 81L45 95H75L70 81" fill="#4A5568" stroke="#000" stroke-width="1.5"/>
            <ellipse cx="60" cy="95" rx="18" ry="3" fill="#2D3748" stroke="#000" stroke-width="1"/>
          </svg>
        {%- endif -%}
      </div>
    </div>
  </div>
</section>

{% stylesheet %}
.about-us {
  position: relative;
  min-height: 100vh;
  background-color: var(--color-white);
  padding: 123px 0 0;
  overflow: hidden;
}

.about-us__grid-pattern {
  position: absolute;
  width: 1443px;
  height: 824px;
  opacity: 0.42;
  pointer-events: none;
  z-index: 0;
}

.about-us__grid-pattern--top {
  top: 5.6%;
  left: -0.21%;
  background-image: 
    repeating-linear-gradient(0deg, transparent, transparent 20px, rgba(0, 110, 216, 0.1) 20px, rgba(0, 110, 216, 0.1) 21px),
    repeating-linear-gradient(90deg, transparent, transparent 20px, rgba(0, 110, 216, 0.1) 20px, rgba(0, 110, 216, 0.1) 21px);
}

.about-us__grid-pattern--bottom {
  top: 42.58%;
  left: -0.49%;
  width: 1443px;
  height: 1281px;
  background-image: 
    repeating-linear-gradient(0deg, transparent, transparent 20px, rgba(0, 110, 216, 0.1) 20px, rgba(0, 110, 216, 0.1) 21px),
    repeating-linear-gradient(90deg, transparent, transparent 20px, rgba(0, 110, 216, 0.1) 20px, rgba(0, 110, 216, 0.1) 21px);
}

.about-us__container {
  position: relative;
  max-width: var(--container-max);
  margin: 0 auto;
  padding: 0 var(--container-padding);
  z-index: 1;
}

/* Hero Section */
.about-us__hero {
  position: relative;
  display: grid;
  grid-template-columns: 1fr 1fr;
  gap: 72px;
  align-items: center;
  margin-bottom: 120px;
  min-height: 830px;
}

.about-us__hero-content {
  display: flex;
  flex-direction: column;
  gap: 48px;
}

.about-us__hero-text {
  display: flex;
  flex-direction: column;
  gap: 14px;
}

.about-us__hero-title {
  font-family: var(--font-heading);
  font-size: 59.468px;
  line-height: 1.31;
  margin: 0;
  display: flex;
  flex-direction: column;
}

.about-us__hero-title--blue {
  color: #006ed8;
  font-weight: 700;
}

.about-us__hero-title--black {
  color: var(--color-black);
  font-weight: 600;
}

.about-us__hero-description {
  font-family: var(--font-heading);
  font-size: 20.298px;
  line-height: 28.998px;
  color: #666;
  max-width: 593px;
  margin: 0;
}

.about-us__hero-button {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  padding: 11.778px 30.917px;
  background-color: var(--color-white);
  border: 0.736px solid var(--color-black);
  border-radius: 3px;
  box-shadow: 4.417px 4.417px 0px 0px var(--color-black);
  font-family: var(--font-product);
  font-weight: 500;
  font-size: 20.61px;
  line-height: 29.444px;
  color: var(--color-black);
  text-decoration: none;
  width: fit-content;
  transition: transform var(--transition-fast), box-shadow var(--transition-fast);
}

.about-us__hero-button:hover {
  transform: translate(-2px, -2px);
  box-shadow: 6px 6px 0px 0px var(--color-black);
}

.about-us__brands {
  display: flex;
  flex-direction: column;
  gap: 13px;
  max-width: 606px;
}

.about-us__brands-title {
  font-family: var(--font-heading);
  font-weight: 500;
  font-size: 20.298px;
  line-height: 28.998px;
  color: #585858;
  margin: 0;
}

.about-us__brands-list {
  display: flex;
  gap: 33px;
  flex-wrap: wrap;
}

.about-us__brand-item {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 4px 18px;
  background-color: rgba(114, 114, 114, 0);
  border: 1px solid #c8c8c8;
  border-radius: 5px;
}

.about-us__brand-logo {
  max-width: 100%;
  height: auto;
  display: block;
}

.about-us__brand-placeholder {
  font-family: var(--font-heading);
  font-weight: 400;
  font-size: 20.298px;
  line-height: 28.998px;
  color: #7c7c7c;
  text-transform: capitalize;
}

.about-us__hero-visual {
  position: relative;
  height: 830px;
}

.about-us__hero-image-wrapper {
  position: absolute;
  top: 55px;
  right: 0;
  width: 495px;
  height: 573px;
}

.about-us__hero-image {
  width: 100%;
  height: 100%;
  object-fit: cover;
  display: block;
}

.about-us__hero-placeholder {
  width: 100%;
  height: 100%;
  background-color: #d9d9d9;
  box-shadow: 4px 4px 0px 0px var(--color-black);
}

/* Franchise Card */
.about-us__franchise-card {
  position: absolute;
  top: 211px;
  right: 0;
  width: 234px;
  background-color: var(--color-white);
  padding: 27px 19px;
  z-index: 2;
}

.about-us__franchise-card-content {
  display: flex;
  flex-direction: column;
  gap: 23px;
}

.about-us__franchise-card-title {
  font-family: var(--font-heading);
  font-weight: 500;
  font-size: 18.402px;
  line-height: 1.31;
  color: var(--color-black);
  margin: 0;
}

.about-us__franchise-card-description {
  font-family: var(--font-heading);
  font-weight: 400;
  font-size: 12.574px;
  line-height: 1.31;
  color: var(--color-black);
  margin: 0;
}

.about-us__franchise-card-features {
  list-style: none;
  padding: 0;
  margin: 0;
  display: flex;
  flex-direction: column;
  gap: 8px;
}

.about-us__franchise-card-feature {
  display: flex;
  align-items: center;
  gap: 3px;
  font-family: var(--font-heading);
  font-weight: 400;
  font-size: 13.424px;
  line-height: 1.31;
  color: var(--color-black);
}

.about-us__checkmark-icon {
  flex-shrink: 0;
  width: 10px;
  height: 10px;
}

.about-us__franchise-card-button {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  padding: 10px 54px;
  background-color: #494949;
  border-radius: 3px;
  font-family: var(--font-accent);
  font-weight: 500;
  font-size: 12px;
  line-height: 1.31;
  color: var(--color-white);
  text-decoration: none;
  text-transform: capitalize;
  width: fit-content;
  transition: background-color var(--transition-fast);
}

.about-us__franchise-card-button:hover {
  background-color: #3a3a3a;
}

/* Decorative Elements */
.about-us__decorative-dot {
  position: absolute;
  width: 6px;
  height: 6px;
  background-color: var(--color-black);
  border-radius: 50%;
  z-index: 1;
}

.about-us__decorative-dot--1 {
  top: 231px;
  right: 729px;
}

.about-us__decorative-dot--2 {
  top: 281px;
  right: 679px;
}

.about-us__decorative-dot--3 {
  top: 382px;
  right: 779px;
}

.about-us__decorative-square {
  position: absolute;
  width: 50px;
  height: 50px;
  background-color: #cce6ff;
  z-index: 1;
}

.about-us__decorative-square--1 {
  top: 302px;
  right: 751px;
}

.about-us__decorative-square--2 {
  top: 483px;
  right: 876px;
}

/* Expertise Section */
.about-us__expertise {
  position: relative;
  margin-top: 120px;
  padding-bottom: 200px;
}

.about-us__expertise-header {
  text-align: center;
  margin-bottom: 48px;
}

.about-us__expertise-title {
  font-family: var(--font-heading);
  font-weight: 600;
  font-size: 29.734px;
  line-height: 1.31;
  color: var(--color-black);
  margin: 0 0 14px;
}

.about-us__expertise-description {
  font-family: var(--font-heading);
  font-weight: 400;
  font-size: 29.734px;
  line-height: 1.31;
  color: #7c7c7c;
  margin: 0;
}

.about-us__expertise-content {
  display: flex;
  flex-direction: column;
  gap: 17px;
  max-width: 1110px;
  margin: 0 auto;
}

.about-us__expertise-row {
  display: grid;
  grid-template-columns: 440px 1fr;
  gap: 72px;
  align-items: center;
}

.about-us__expertise-row--reverse {
  grid-template-columns: 1fr 440px;
}

.about-us__expertise-row--reverse .about-us__expertise-image-wrapper {
  order: 2;
}

.about-us__expertise-row--reverse .about-us__expertise-text {
  order: 1;
}

.about-us__expertise-image-wrapper {
  width: 440px;
  height: 279px;
}

.about-us__expertise-image {
  width: 100%;
  height: 100%;
  object-fit: cover;
  display: block;
}

.about-us__expertise-placeholder {
  width: 100%;
  height: 100%;
  background-color: #d9d9d9;
  box-shadow: 4px 4px 0px 0px #74a7ff;
}

.about-us__expertise-text {
  font-family: var(--font-heading);
  font-weight: 400;
  font-size: 24px;
  line-height: 1.31;
  color: #7c7c7c;
}

.about-us__expertise-text p {
  margin: 0 0 23px;
}

.about-us__expertise-text p:last-child {
  margin-bottom: 0;
}

.about-us__expertise-link {
  font-family: var(--font-heading);
  font-weight: 400;
  font-size: 24px;
  line-height: 1.31;
  color: #57a0ff;
  text-decoration: none;
}

.about-us__expertise-link:hover {
  text-decoration: underline;
}

.about-us__laptop-illustration {
  position: absolute;
  bottom: 0;
  right: 0;
  width: 183.478px;
  height: 170.688px;
  display: flex;
  align-items: center;
  justify-content: center;
  transform: rotate(355.527deg);
}

.about-us__laptop-img {
  width: 100%;
  height: auto;
  display: block;
}

/* Responsive Design */
@media screen and (max-width: 1200px) {
  .about-us__hero {
    grid-template-columns: 1fr;
    gap: 40px;
    min-height: auto;
  }
  
  .about-us__hero-visual {
    height: auto;
    min-height: 500px;
  }
  
  .about-us__expertise-row {
    grid-template-columns: 1fr;
    gap: 40px;
  }
  
  .about-us__expertise-row--reverse {
    grid-template-columns: 1fr;
  }
  
  .about-us__expertise-row--reverse .about-us__expertise-image-wrapper,
  .about-us__expertise-row--reverse .about-us__expertise-text {
    order: initial;
  }
  
  .about-us__expertise-image-wrapper {
    width: 100%;
    max-width: 440px;
    margin: 0 auto;
  }
}

@media screen and (max-width: 768px) {
  .about-us {
    padding: 80px 0 0;
  }
  
  .about-us__hero-title {
    font-size: 36px;
  }
  
  .about-us__hero-description {
    font-size: 16px;
    line-height: 1.5;
  }
  
  .about-us__hero-button {
    font-size: 16px;
    padding: 10px 24px;
  }
  
  .about-us__brands-title {
    font-size: 16px;
  }
  
  .about-us__brand-placeholder {
    font-size: 14px;
  }
  
  .about-us__hero-image-wrapper {
    width: 100%;
    max-width: 495px;
    position: relative;
    top: 0;
    right: auto;
    margin: 0 auto;
  }
  
  .about-us__franchise-card {
    position: relative;
    top: 0;
    right: auto;
    width: 100%;
    max-width: 234px;
    margin: 20px auto 0;
  }
  
  .about-us__decorative-dot,
  .about-us__decorative-square {
    display: none;
  }
  
  .about-us__expertise-title {
    font-size: 24px;
  }
  
  .about-us__expertise-description {
    font-size: 18px;
  }
  
  .about-us__expertise-text {
    font-size: 18px;
  }
  
  .about-us__laptop-illustration {
    position: relative;
    margin: 40px auto 0;
    width: 120px;
    height: 112px;
  }
}

@media screen and (max-width: 480px) {
  .about-us__hero-title {
    font-size: 28px;
  }
  
  .about-us__brands-list {
    gap: 16px;
  }
  
  .about-us__expertise-content {
    gap: 32px;
  }
}
{% endstylesheet %}

