{%- liquid
  assign hero_title_line_1 = section.settings.hero_title_line_1 | default: 'Beyond Refurbishment'
  assign hero_title_line_2 = section.settings.hero_title_line_2 | default: 'Redefining Smart  Tech Ownership'
  assign hero_description = section.settings.hero_description | default: 'ReGuru is building a sustainable, circular tech ecosystem — making technology accessible while reducing e-waste.'
  assign brands_title = section.settings.brands_title | default: 'Top Brands  we sell:'
  
  assign tech_standards_title = section.settings.tech_standards_title | default: 'Tech standards'
  assign tech_standards_description = section.settings.tech_standards_description | default: 'Our commitment to excellence ensures every device leaving out faiclity meets the highest industrial performance benchmanrks'
  
  assign quality_title = section.settings.quality_title | default: 'Uncompromising Quality'
  assign quality_description = section.settings.quality_description | default: 'Every device undergoes  45-point diagnostic check,including battery health, pixel density, and neural board stability'
  
  assign eco_title = section.settings.eco_title | default: 'Eco- First Lifecycle'
  assign eco_description = section.settings.eco_description | default: 'We operate on a zero-waste policy. Parts that cannot be refurbished are ethically recycled.'
  
  assign affordability_title = section.settings.affordability_title | default: 'Affordability'
  assign affordability_description = section.settings.affordability_description | default: 'Premium technology should be accessible. We offer flagship devices at upto 60% less than original retail price'
  
  assign grow_title = section.settings.grow_title | default: 'Grow with Us'
  assign grow_description = section.settings.grow_description | default: 'Become a part of the circular economy revolution. Our franchise program provides you with the inventory, technical training, and diagnostic software needed to launch your own refurbishment center.'
  assign grow_feature_1 = section.settings.grow_feature_1 | default: 'Access to global refurbished inventory pipelines'
  assign grow_feature_2 = section.settings.grow_feature_2 | default: 'Proprietary OS diagnostic tools suite'
  assign grow_feature_3 = section.settings.grow_feature_3 | default: 'Certified technician training & certification'
  assign grow_button_text = section.settings.grow_button_text | default: 'Join Franchise'
  assign grow_button_link = section.settings.grow_button_link | default: '#'
  assign grow_image = section.settings.grow_image
  
  assign image_card_1 = section.settings.image_card_1
  assign image_card_2 = section.settings.image_card_2
  assign image_card_3 = section.settings.image_card_3
-%}

<section class="about-us" data-section-id="{{ section.id }}">
  <div class="about-us__container">
    <!-- Hero Section -->
    <div class="about-us__hero">
      <div class="about-us__hero-content">
        <div class="about-us__hero-text">
          <h1 class="about-us__hero-title">
            <span class="about-us__hero-title--blue">{{ hero_title_line_1 }}</span>
            <span class="about-us__hero-title--black">{{ hero_title_line_2 }}</span>
          </h1>
          <p class="about-us__hero-description">{{ hero_description }}</p>
        </div>
        
        <div class="about-us__brands">
          <p class="about-us__brands-title">{{ brands_title }}</p>
          <div class="about-us__brands-list">
            {%- for block in section.blocks -%}
              {%- if block.type == 'brand_logo' -%}
                <div class="about-us__brand-item" {{ block.shopify_attributes }}>
                  {%- if block.settings.brand_logo != blank -%}
                    <img 
                      src="{{ block.settings.brand_logo | image_url: width: 120 }}"
                      alt="{{ block.settings.brand_name | default: 'Brand logo' }}"
                      width="120"
                      height="40"
                      loading="lazy"
                      class="about-us__brand-logo"
                    >
                  {%- else -%}
                    <span class="about-us__brand-placeholder">{{ block.settings.brand_name | default: 'logo ' | append: forloop.index }}</span>
                  {%- endif -%}
                </div>
              {%- endif -%}
            {%- endfor -%}
            
            {%- if section.blocks.size == 0 -%}
              {%- assign default_brands = 'logo one|logo two|logo three|logo four' | split: '|' -%}
              {%- for brand in default_brands -%}
                <div class="about-us__brand-item">
                  <span class="about-us__brand-placeholder">{{ brand }}</span>
                </div>
              {%- endfor -%}
            {%- endif -%}
          </div>
        </div>
      </div>
      
      <div class="about-us__hero-visual">
        <div class="about-us__image-grid">
          {%- if image_card_1 != blank -%}
            <div class="about-us__image-card about-us__image-card--1">
              <img 
                src="{{ image_card_1 | image_url: width: 381 }}"
                alt="{{ image_card_1.alt | default: 'About us image' }}"
                width="381"
                height="205"
                loading="lazy"
                class="about-us__image-card-img"
              >
            </div>
          {%- else -%}
            <div class="about-us__image-card about-us__image-card--1 about-us__image-card--placeholder"></div>
          {%- endif -%}
          
          {%- if image_card_2 != blank -%}
            <div class="about-us__image-card about-us__image-card--2">
              <img 
                src="{{ image_card_2 | image_url: width: 347 }}"
                alt="{{ image_card_2.alt | default: 'About us image' }}"
                width="347"
                height="205"
                loading="lazy"
                class="about-us__image-card-img"
              >
            </div>
          {%- else -%}
            <div class="about-us__image-card about-us__image-card--2 about-us__image-card--placeholder"></div>
          {%- endif -%}
          
          {%- if image_card_3 != blank -%}
            <div class="about-us__image-card about-us__image-card--3">
              <img 
                src="{{ image_card_3 | image_url: width: 255 }}"
                alt="{{ image_card_3.alt | default: 'About us image' }}"
                width="255"
                height="193"
                loading="lazy"
                class="about-us__image-card-img"
              >
            </div>
          {%- else -%}
            <div class="about-us__image-card about-us__image-card--3 about-us__image-card--placeholder"></div>
          {%- endif -%}
        </div>
      </div>
    </div>
    
    <!-- Tech Standards Section -->
    <div class="about-us__tech-standards">
      <h2 class="about-us__tech-standards-title">{{ tech_standards_title }}</h2>
      <p class="about-us__tech-standards-description">{{ tech_standards_description }}</p>
      
      <div class="about-us__feature-cards">
        <div class="about-us__feature-card">
          <div class="about-us__feature-card-icon about-us__feature-card-icon--quality">
            <img 
              src="{{ 'about-quality-icon.png' | asset_url }}"
              alt="Quality icon"
              width="154"
              height="137"
              loading="lazy"
            >
          </div>
          <h3 class="about-us__feature-card-title">{{ quality_title }}</h3>
          <p class="about-us__feature-card-description">{{ quality_description }}</p>
        </div>
        
        <div class="about-us__feature-card">
          <div class="about-us__feature-card-icon about-us__feature-card-icon--eco">
            <img 
              src="{{ 'about-eco-icon.png' | asset_url }}"
              alt="Eco icon"
              width="127"
              height="137"
              loading="lazy"
            >
          </div>
          <h3 class="about-us__feature-card-title">{{ eco_title }}</h3>
          <p class="about-us__feature-card-description">{{ eco_description }}</p>
        </div>
        
        <div class="about-us__feature-card">
          <div class="about-us__feature-card-icon about-us__feature-card-icon--affordability">
            <img 
              src="{{ 'about-affordability-icon.png' | asset_url }}"
              alt="Affordability icon"
              width="132"
              height="137"
              loading="lazy"
            >
          </div>
          <h3 class="about-us__feature-card-title">{{ affordability_title }}</h3>
          <p class="about-us__feature-card-description">{{ affordability_description }}</p>
        </div>
      </div>
    </div>
    
    <!-- Grow with Us Section -->
    <div class="about-us__grow">
      <div class="about-us__grow-content">
        <div class="about-us__grow-text">
          <h2 class="about-us__grow-title">{{ grow_title }}</h2>
          <p class="about-us__grow-description">{{ grow_description }}</p>
          
          <ul class="about-us__grow-features">
            {%- if grow_feature_1 != blank -%}
              <li class="about-us__grow-feature">{{ grow_feature_1 }}</li>
            {%- endif -%}
            {%- if grow_feature_2 != blank -%}
              <li class="about-us__grow-feature">{{ grow_feature_2 }}</li>
            {%- endif -%}
            {%- if grow_feature_3 != blank -%}
              <li class="about-us__grow-feature">{{ grow_feature_3 }}</li>
            {%- endif -%}
          </ul>
          
          <a href="{{ grow_button_link }}" class="about-us__grow-button">
            {{ grow_button_text }}
          </a>
        </div>
        
        <div class="about-us__grow-image-wrapper">
          {%- if grow_image != blank -%}
            <img 
              src="{{ grow_image | image_url: width: 394 }}"
              alt="{{ grow_image.alt | default: 'Franchise image' }}"
              width="394"
              height="424"
              loading="lazy"
              class="about-us__grow-image"
            >
          {%- else -%}
            <div class="about-us__grow-placeholder"></div>
          {%- endif -%}
        </div>
      </div>
    </div>
  </div>
</section>

{% stylesheet %}
.about-us {
  position: relative;
  background-color: rgba(255, 255, 255, 0);
  color: rgba(0, 0, 0, 0);
  padding: 25px 0 4px;
  overflow: visible;
}

.about-us__container {
  position: relative;
  max-width: var(--container-max);
  margin: 0 auto;
  padding: 0 var(--container-padding);
  color: rgba(0, 0, 0, 0);
}

/* Hero Section */
.about-us__hero {
  position: relative;
  display: flex;
  align-items: flex-start;
  gap: 0px;
  padding: 25px 64px 0;
  margin-bottom: 120px;
}

.about-us__hero-content {
  display: flex;
  flex-direction: column;
  gap: 30px;
  flex: 0 0 755px;
}

.about-us__hero-text {
  display: flex;
  flex-direction: column;
  gap: 30px;
}

.about-us__hero-title {
  font-family: var(--font-heading);
  font-size: 59.468px;
  line-height: 1.31;
  margin: 0;
  display: flex;
  flex-direction: column;
  gap: 0;
}

.about-us__hero-title--blue {
  color: #006ed8;
  font-weight: 700;
}

.about-us__hero-title--black {
  color: var(--color-black);
  font-size: 32px;
  font-weight: 600;
}

.about-us__hero-description {
  font-family: var(--font-heading);
  font-size: 20.298px;
  line-height: 28.998px;
  color: #1e1a5c;
  max-width: 598px;
  margin: 0;
}

.about-us__brands {
  display: flex;
  flex-direction: column;
  gap: 0;
  max-width: 606px;
}

.about-us__brands-title {
  font-family: var(--font-heading);
  font-weight: 500;
  font-size: 20.298px;
  line-height: 28.998px;
  color: #585858;
  margin: 0 0 0;
}

.about-us__brands-list {
  display: flex;
  gap: 33px;
  flex-wrap: wrap;
  margin-top: 0;
}

.about-us__brand-item {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 4px 18px;
  background-color: rgba(114, 114, 114, 0);
  border: 1px solid rgba(255, 255, 255, 1);
  border-radius: 5px;
}

.about-us__brand-logo {
  max-width: 100%;
  height: auto;
  display: block;
}

.about-us__brand-placeholder {
  font-family: var(--font-heading);
  font-weight: 400;
  font-size: 20.298px;
  line-height: 28.998px;
  color: #7c7c7c;
  text-transform: capitalize;
}

/* Hero Visual - Image Grid */
.about-us__hero-visual {
  position: relative;
  flex: 1;
  min-height: 441px;
}

.about-us__image-grid {
  position: relative;
  width: 100%;
  height: 100%;
}

.about-us__image-card {
  position: absolute;
  background-color: #d9d9d9;
  border-radius: 12px;
  box-shadow: 2.615px 2.615px 0px 0px var(--color-black);
  overflow: hidden;
}

.about-us__image-card--1 {
  width: 381px;
  height: 205px;
  left: 165px;
  top: 0;
  z-index: 3;
}

.about-us__image-card--2 {
  width: 347px;
  height: 205px;
  left: 290px;
  top: 236px;
  z-index: 2;
}

.about-us__image-card--3 {
  width: 255px;
  height: 193px;
  left: 0;
  top: 225px;
  z-index: 1;
}

.about-us__image-card-img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  display: block;
}

.about-us__image-card--placeholder {
  background-color: #d9d9d9;
}

/* Tech Standards Section */
.about-us__tech-standards {
  text-align: center;
  margin-bottom: 120px;
}

.about-us__tech-standards-title {
  font-family: var(--font-heading);
  font-weight: 500;
  font-size: 36px;
  line-height: 28.998px;
  color: var(--color-black);
  text-transform: capitalize;
  margin: 0 0 10px;
}

.about-us__tech-standards-description {
  font-family: var(--font-heading);
  font-weight: 400;
  font-size: 20px;
  line-height: 28.998px;
  color: var(--color-black);
  margin: 0 0 40px;
  max-width: 800px;
  margin-left: auto;
  margin-right: auto;
}

.about-us__feature-cards {
  display: flex;
  gap: 35px;
  justify-content: center;
  align-items: flex-start;
  flex-wrap: wrap;
}

.about-us__feature-card {
  position: relative;
  width: 386px;
  height: 358px;
  background-color: var(--color-white);
  border-radius: 6px;
  box-shadow: 4px 4px 0px 0px var(--color-black);
  padding: 26px 33px 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
  flex-shrink: 0;
}

.about-us__feature-card-icon {
  height: 137px;
  margin-bottom: 29px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.about-us__feature-card-icon--quality {
  width: 154.125px;
}

.about-us__feature-card-icon--eco {
  width: 127px;
}

.about-us__feature-card-icon--affordability {
  width: 132px;
}

.about-us__feature-card-icon img {
  width: 100%;
  height: auto;
  object-fit: contain;
}

.about-us__feature-card-title {
  font-family: var(--font-heading);
  font-weight: 600;
  font-size: 20px;
  line-height: 28.998px;
  color: var(--color-black);
  margin: 0 0 8px;
  min-height: 29px;
}

.about-us__feature-card-description {
  font-family: var(--font-heading);
  font-weight: 400;
  font-size: 20px;
  line-height: 28.998px;
  color: var(--color-black);
  margin: 0;
  max-width: 320px;
}

/* Grow with Us Section */
.about-us__grow {
  background-color: #b1d8ff;
  padding: 89px 0;
  margin-top: 68px;
  margin-bottom: 68px;
  margin-left: calc(-1 * var(--container-padding));
  margin-right: calc(-1 * var(--container-padding));
  box-shadow: 4px 4px 0px 0px rgba(0, 0, 0, 0.73);
  border-radius: 12px;
}

.about-us__grow-content {
  max-width: var(--container-max);
  margin: 0 auto;
  padding: 0 var(--container-padding);
  display: flex;
  gap: 140px;
  align-items: center;
}

.about-us__grow-text {
  display: flex;
  flex-direction: column;
  gap: 43px;
  flex: 0 0 690px;
}

.about-us__grow-title {
  font-family: var(--font-product);
  font-weight: 500;
  font-size: 61.146px;
  line-height: 87.352px;
  color: var(--color-black);
  margin: 0;
}

.about-us__grow-description {
  font-family: var(--font-product);
  font-weight: 500;
  font-size: 20.611px;
  line-height: 29.444px;
  color: var(--color-black);
  margin: 0;
}

.about-us__grow-features {
  list-style: none;
  padding: 0;
  margin: 0;
  display: flex;
  flex-direction: column;
  gap: 12px;
}

.about-us__grow-feature {
  font-family: var(--font-product);
  font-weight: 500;
  font-size: 20.611px;
  line-height: 29.444px;
  color: var(--color-black);
}

.about-us__grow-button {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  padding: 11.778px 30.917px;
  background-color: var(--color-green);
  border: 0.736px solid var(--color-black);
  border-radius: 6px;
  box-shadow: 4.417px 4.417px 0px 0px var(--color-black);
  font-family: var(--font-product);
  font-weight: 500;
  font-size: 20.61px;
  line-height: 29.444px;
  color: var(--color-black);
  text-decoration: none;
  width: fit-content;
  transition: transform var(--transition-fast), box-shadow var(--transition-fast);
}

.about-us__grow-button:hover {
  transform: translate(-2px, -2px);
  box-shadow: 6px 6px 0px 0px var(--color-black);
}

.about-us__grow-image-wrapper {
  flex: 0 0 394px;
  height: 424px;
}

.about-us__grow-image {
  width: 100%;
  height: 100%;
  object-fit: cover;
  border: 1px solid var(--color-black);
  border-radius: 8px;
  box-shadow: 2px 2px 0px 0px var(--color-black);
  display: block;
}

.about-us__grow-placeholder {
  width: 100%;
  height: 100%;
  background-color: #d9d9d9;
  border: 1px solid var(--color-black);
  border-radius: 8px;
  box-shadow: 2px 2px 0px 0px var(--color-black);
}

/* Responsive Design */
@media screen and (max-width: 1400px) {
  .about-us__hero {
    gap: 60px;
    padding: 0 40px;
  }
  
  .about-us__hero-content {
    flex: 0 0 auto;
    max-width: 600px;
  }
  
  .about-us__grow-content {
    gap: 80px;
  }
  
  .about-us__grow-text {
    flex: 0 0 auto;
    max-width: 600px;
  }
}

@media screen and (max-width: 1200px) {
  .about-us__hero {
    flex-direction: column;
    gap: 60px;
    padding: 0 var(--container-padding);
  }
  
  .about-us__hero-content {
    width: 100%;
    max-width: 100%;
  }
  
  .about-us__hero-visual {
    width: 100%;
    min-height: 400px;
  }
  
  .about-us__image-card--1 {
    left: 10%;
  }
  
  .about-us__image-card--2 {
    left: 20%;
  }
  
  .about-us__image-card--3 {
    left: 0;
  }
  
  .about-us__feature-cards {
    gap: 30px;
  }
  
  .about-us__feature-card {
    width: calc(33.333% - 24px);
    min-width: 300px;
  }
  
  .about-us__grow-content {
    flex-direction: column;
    gap: 40px;
  }
  
  .about-us__grow-text {
    width: 100%;
    max-width: 100%;
  }
  
  .about-us__grow-image-wrapper {
    width: 100%;
    max-width: 394px;
    margin: 0 auto;
  }
}

@media screen and (max-width: 992px) {
  .about-us__feature-card {
    width: calc(50% - 18px);
    min-width: 300px;
  }
}

@media screen and (max-width: 768px) {
  .about-us {
    padding: 20px 0 0;
  }
  
  .about-us__hero {
    padding: 0 var(--container-padding);
    margin-bottom: 80px;
  }
  
  .about-us__hero-title {
    font-size: 36px;
  }
  
  .about-us__hero-title--black {
    font-size: 24px;
  }
  
  .about-us__hero-description {
    font-size: 18px;
    line-height: 1.5;
  }
  
  .about-us__brands-title {
    font-size: 18px;
  }
  
  .about-us__brand-placeholder {
    font-size: 16px;
  }
  
  .about-us__hero-visual {
    min-height: 300px;
  }
  
  .about-us__image-card--1,
  .about-us__image-card--2,
  .about-us__image-card--3 {
    position: relative;
    left: auto;
    top: auto;
    width: 100%;
    max-width: 300px;
    margin: 0 auto 20px;
  }
  
  .about-us__tech-standards {
    margin-bottom: 60px;
  }
  
  .about-us__tech-standards-title {
    font-size: 28px;
  }
  
  .about-us__tech-standards-description {
    font-size: 18px;
    padding: 0 var(--container-padding);
  }
  
  .about-us__feature-cards {
    flex-direction: column;
    align-items: center;
    padding: 0 var(--container-padding);
  }
  
  .about-us__feature-card {
    width: 100%;
    max-width: 386px;
    min-width: auto;
  }
  
  .about-us__grow {
    padding: 60px 0;
  }
  
  .about-us__grow-title {
    font-size: 36px;
    line-height: 1.3;
  }
  
  .about-us__grow-description {
    font-size: 18px;
    line-height: 1.5;
  }
  
  .about-us__grow-feature {
    font-size: 18px;
    line-height: 1.5;
  }
  
  .about-us__grow-button {
    font-size: 18px;
    padding: 10px 24px;
  }
}

@media screen and (max-width: 480px) {
  .about-us__hero-title {
    font-size: 28px;
  }
  
  .about-us__hero-title--black {
    font-size: 20px;
  }
  
  .about-us__brands-list {
    gap: 16px;
  }
  
  .about-us__tech-standards-title {
    font-size: 24px;
  }
  
  .about-us__grow-title {
    font-size: 28px;
  }
}
{% endstylesheet %}

{% schema %}
{
  "name": "About Us",
  "tag": "section",
  "class": "about-us-section",
  "settings": [
    {
      "type": "header",
      "content": "Hero Section"
    },
    {
      "type": "text",
      "id": "hero_title_line_1",
      "label": "Title Line 1",
      "default": "Beyond Refurbishment"
    },
    {
      "type": "text",
      "id": "hero_title_line_2",
      "label": "Title Line 2",
      "default": "Redefining Smart  Tech Ownership"
    },
    {
      "type": "textarea",
      "id": "hero_description",
      "label": "Description",
      "default": "ReGuru is building a sustainable, circular tech ecosystem — making technology accessible while reducing e-waste."
    },
    {
      "type": "text",
      "id": "brands_title",
      "label": "Brands Title",
      "default": "Top Brands  we sell:"
    },
    {
      "type": "image_picker",
      "id": "image_card_1",
      "label": "Image Card 1 (Top Right)"
